        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Set location, destruction, and firing movement
//Set the x and y to move with the main boss, if drill has not been fired
if !fired
    {
    x = obj_boss2.x + 74; 
    y = obj_boss2.y + 18; 
    }
    
//If HP &lt;= 0, then create explosion object and destroy self
if hp &lt;= 0
    {
    instance_create(x, y, obj_explosion); 
    
    //set the boss's path speed back to 4, in case the drill in destroyed while fired
    with(obj_boss2)
        {
        path_speed = 4;
        }
    instance_destroy();
    } 
    
//If drill has been fired, and has reached the bottom of screen, then stop movement
if fired &amp;&amp; y &gt;= 415
    {
    if !bottom
        {
        speed = 0;
        bottom = true;
        //play drill hit sound
        audio_play_sound(snd_mining_boss_drill_hit, 1, false); 
        }  
    }
    
    
//if drill has been fired and has reached the starting point, then stop movement
if fired &amp;&amp; y &lt;= obj_boss2.y
    {
    if bottom
        {
        speed = 0; 
        bottom = false; 
        }
    }
    

</string>
          </argument>
        </arguments>
      </action>
