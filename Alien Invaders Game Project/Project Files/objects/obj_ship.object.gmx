            <string>///Firing Lasers

//Creates an instance of the laser object, but only if the global shots fired variable 
//less than 3. Ensuring that there can only be 3 laser instances at once. The variable
//is decreased each time a laser is destroyed. 
if global.shots_fired &lt; 3
    {
    //if no double or spread shots available, or player is in transition or destroyed room then fire single laser 
    if (global.doublelaser &lt;= 0 &amp;&amp; global.spreadlaser &lt;= 0) || room == rm_transition || room == rm_destroyed
        {    
        laser = instance_create(x, y - 30, obj_laser); 
        //set laser's direction as UP and speed at 15
        laser.direction = 90;
        laser.speed = 10; 
        }
    
  
    }

//if player is not in the transition room or destroyed room, then allow double and spread shots. 
if room != rm_transition &amp;&amp; room != rm_destroyed
{
                  
         //if the spread laser counter is greater than zero and there are no double lasers, create a spread laser shot
         if global.spreadlaser &gt; 0 &amp;&amp; global.doublelaser &lt;= 0 
            {
            //create the first laser to travel at 60 degrees at speed 15
            spreadfirst = instance_create(x, y - 30, obj_laser);
            spreadfirst.image_angle = 330; 
            spreadfirst.direction = 60;
            spreadfirst.speed = 15; 
         
        
            //create the 2nd laser to travel at 75 degrees at speed 15
            spreadsecond = instance_create(x, y - 30, obj_laser);
            spreadsecond.image_angle = 345; 
            spreadsecond.direction = 75;
            spreadsecond.speed = 15; 
        
                
            //create the 3rd laser to travel at 90 degrees at speed 15
            spreadthird = instance_create(x, y - 30, obj_laser);
            spreadthird.image_angle = 0; 
            spreadthird.direction = 90;
            spreadthird.speed = 15; 
            
           
            //create the 4th laser to travel at 105 degrees at speed 15
            spreadfourth = instance_create(x, y - 30, obj_laser);
            spreadfourth.image_angle = 15; 
            spreadfourth.direction = 105;
            spreadfourth.speed = 15; 
                        
            //create the 5th laser to travel at 120 degrees at speed 15
            spreadfifth = instance_create(x, y - 30, obj_laser);
            spreadfifth.image_angle = 30; 
            spreadfifth.direction = 120;
            spreadfifth.speed = 15; 
                        
            global.spreadlaser--;     
            }
            
        //If there are double laser shots available in the global variable, then create
        // two parrallel laser objects
        if global.doublelaser &gt; 0
            {
            //create the left laser at -10 of ship's center
            doublefirst = instance_create(x - 10, y - 30, obj_laser); 
            doublefirst.direction = 90;
            doublefirst.speed = 15;     
                        
            //create the right laser at +10 of ship's center
            doublesecond = instance_create(x + 10, y - 30, obj_laser); 
            //set laser's vertical speed to be -15 so it moves up
            doublesecond.direction = 90;
            doublesecond.speed = 15; 
                            
                //decreae the globale doube laser counter
            global.doublelaser--; 
            }
   }   
                    
            

</string>
