        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Movement &amp; HP &lt;=0

if hp &lt;= 0 &amp;&amp; !destroyed
    {
    //Set destroyed variable to true, to stop movement
    destroyed = true;
    
    //set alarm 0 to create explosion at trigger room fade
    alarm_set(0, 1);
    } 


//if not destroyed, then set image index to 0
if !destroyed
    {
    image_index = 0;
    } 

//Check if left key is pressed and ship is not destroyed, and if so move the ship to the left 8 pixels
//only if x is greater than 0
if keyboard_check(vk_left) &amp;&amp; !destroyed
    {
    image_index = 2; 
    if x &gt; 0 x -= 8; 
    }
    
//Check if right is pressed and ship is not destroyed, and if so move the ship to the right 8 pixels  
// only if x is less than 640
if keyboard_check(vk_right) &amp;&amp; !destroyed
    {
    image_index = 1; 
    if x &lt; 640 x += 8;
    }
    


      

</string>
          </argument>
        </arguments>
      </action>
    </event>
