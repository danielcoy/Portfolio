    }    

//if starty is positive, then allow to draw across full x range
if starty &lt;= 0
    {
    startx = -200 + random_range(0, 1140); 
    }
    
 

//create an instance of asteroid at the location generated above
//if there are other enemies on the screen
if global.enemies
