        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Destruction, image angle, and laser firing

//destroy instnace if hp 0 or less
if hp &lt;= 0
    {
    instance_destroy();
    }
    
//if within 150 distance to player, play the dive sound if it is not already playing
if distance_to_object(obj_ship) &lt; 150
    {
        if !audio_is_playing(snd_fighter_flyby)
            {
            audio_play_sound(snd_fighter_flyby, 1, false);
            }
    }     

    
//If the direction is between 0 = 90 or 270 - 360 the instance is moving to the right
// and its yscale should be 1        
if (direction &gt; 0 &amp;&amp; direction &lt; 90) || (direction &gt; 270 &amp;&amp; direction &lt; 360)
    {
    image_yscale = 1;
    
    //if moving right and cross the 100 x coordinate, is still on screen, and hasn't fired, then set fire to true
    if x &gt;= 100 &amp;&amp; x &lt; 640 &amp;&amp; !fire
        {
        fire = true;
        
         //create one enemy lasers to fire in the current direction moving
        laser1 = instance_create(x, y - 5, obj_EnemyLaser);
        //**Commenting out the second laser fire because it was causing too much damage
        //laser2 = instance_create(x, y + 5, obj_EnemyLaser);
    
        //set the lasers' directions to the fighter's direction, set speed to 15, 
        //and the image angle to the direction of travel
        with (laser1)
            {
            direction = other.direction; 
            speed = 15;
            image_angle = direction - 90; 
            }
        
        //Commenting out, see above**
        //with (laser2)
         //   {
         //   direction = other.direction; 
          //  speed = 15;
         //   image_angle = direction - 90; 
         //   } 
        }      
    }
 
//If the direction is between 90 - 270, the instnace is moving left and 
// its yscale should be -1, since the image angle will roate it to be upside down,
//this will flip it back upright       
if (direction &gt;= 90 &amp;&amp; direction &lt;= 270)
    {
    image_yscale = -1;
    
    //if moving left and cross the 540 x coordinate, is still on screen, and hasn't fired, then set fire to true
    if x &lt;= 540 &amp;&amp; x &gt; 0 &amp;&amp; !fire
        {
        fire = true;
       
         //create two enemy lasers to fire in the current direction moving
        laser1 = instance_create(x, y - 5, obj_EnemyLaser);
        //Commening out, see above **
        //laser2 = instance_create(x, y + 5, obj_EnemyLaser);
    
        //set the lasers' directions to the fighter's direction, set speed to 15, 
        //and the image angle to the direction of travel
        with (laser1)
            {
            direction = other.direction; 
            speed = 15;
            image_angle = direction - 90; 
            }
        
        //Commenting out, see above**       
         //with (laser2)
         //   {
          //  direction = other.direction; 
          //  speed = 15;
         //   image_angle = direction - 90; 
          //  }
             
        } 
    }

//set the image angle to the direction it is moving        
image_angle = direction; 

//when instance leaves the screen, reset the fire trigger to false for the next pass
if x &lt;= 0 || x &gt;= 640
    {
    fire = false; 
    }
    
    
        
    










</string>
