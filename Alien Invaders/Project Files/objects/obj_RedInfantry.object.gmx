        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Movement of Alien right, left, then down
//////////////////////////////////////////

//if horizontal postion has increaed to 8, i.e. the alien has moved 8 positions 
// to the right, then change moving_right to 0, in order to indicate that it will
// now need to move left. We will also move the alien down one position.  
if hor_pos == 8
    {
    moveing_right = 0;
    y += 16;     
    }
    
//if hor_pos has decresed to -16, then alien has reached left limit and must start 
//moveing right again. Also move alien down one position. 
if hor_pos == -8
    {
    moveing_right = 1; 
    y += 16; 
    }
        

// If moveing_right is 1 and the alien has moved right less than 8 positions, 
// then move right 1 pos and increment the counter
if moveing_right == 1 &amp;&amp; hor_pos &lt; 8
    {
    x += 16; 
    hor_pos++;
    }
    
//If moving_right is set to zero and hor_pos is greater than -16, then the alien needs to move left
// and negative increment the counter
if moveing_right == 0  &amp;&amp; hor_pos &gt; -8
    {
    x -= 16;
    hor_pos--;
    }
    
//reset the alarm to half a second    
alarm_set(0, 15); 
</string>
          </argument>
        </arguments>
      </action>
