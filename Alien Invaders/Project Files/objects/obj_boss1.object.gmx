          <argument>
            <kind>1</kind>
            <string>///When HP &lt; = 0

if hp &lt;= 0 &amp;&amp; !destroyed 
    {
    destroyed = true;
    //set alarm to create explosions every ~1/4 second
    alarm_set(3, 7);
    
    //set alarm to decrease the enemies count and end the level
    alarm_set(4, 180); 
    
    //set the path to pth_destroyed
    path_start(pth_destroyed, 1, path_action_stop, false); 
    
    //remove the shield object
    with (obj_boss1shield)
        {
        instance_destroy();
        }
    
        
    //set image speed to 0 to stop animation
    image_speed = 0; 
            
    //increment score
    score += 5000;
    }


    

</string>
